#3A
# Write a program to copy files using thread

import _thread
import time

def fun1():
    f1 = open("Wildlife.wmv","rb")
    f2 = open("W1.wmv","wb")
    f2.write(f1.read())
    f1.close()
    f2.close()

def fun2():
    f1 = open("robin.mp4","rb")
    f2 = open("robin2.mp4","wb")
    f2.write(f1.read())
    f1.close()
    f2.close()

start_time = time.time()
_thread.start_new_thread(fun1,())
_thread.start_new_thread(fun2,())
print("Time Taken: ",(time.time() - start_time))
__________________________________________________________________________________________________________________________

#3B
# Write a program to demonstrate locks in threads

import _thread
import threading

def fun1(n):
    t.acquire_lock()
    for i in range(1,n):
        print("\nT1")
    t.release_lock()
  
def fun2(n):
    t.acquire_lock()
    for i in range(1,n):
        print("\mT2")
    t.release_lock()
    
start_time = time.time()
_thread.start_new_thread(fun1,(5,))
_thread.start_new_thread(fun2,(4,))
print("Time Taken: ",(time.time() - start_time))
